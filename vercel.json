{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "api/(.*)",
            "dest": "api/index.js"
        }
    ],
    "functions": {
        "api/*.js":{
         "maxDuration":10
        }
     }
}